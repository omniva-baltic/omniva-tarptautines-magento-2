<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <carriers>
            <omnivaglobal>
                <production_webservices_url><![CDATA[https://tarptautines.omniva.lt]]></production_webservices_url>
                <shipment_requesttype>0</shipment_requesttype>
                <active>0</active>
                <sallowspecific>0</sallowspecific>
                <allowed_methods>COURIER,PARCEL_TERMINAL</allowed_methods>
                <model>Omniva\Shipping\Model\Carrier</model>
                <title>Omniva global</title>
                <terminal_title>Parcel terminal</terminal_title>
                <courier_title>Courier</courier_title>
                <services_order>default</services_order>
                <specificerrmsg>This shipping method is currently unavailable. If you would like to ship using this shipping method, please contact us.</specificerrmsg>
                <unit_of_measure>KG</unit_of_measure>
                <max_package_weight>150</max_package_weight>
                <free_shipping_amount>0</free_shipping_amount>
                <product_weight>1</product_weight>
                <product_width>20</product_width>
                <product_height>20</product_height>
                <product_length>20</product_length>
            </omnivaglobal>
        </carriers>
    </default>
</config>
